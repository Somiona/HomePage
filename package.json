    "scripts": {
        "build": "gatsby build",
        "develop": "gatsby develop",
        "start": "yarn run develop",
        "clean": "gatsby clean",
        "type-check": "tsc --noEmit -p .",
        "format": "prettier --write .",
        "lint": "eslint 'src/**' --max-warnings 10",
        "lint-fix": "eslint 'src/**' --fix",
        "gen-scss-type": "yarn tsm src/",
        "serve": "pushd .\\public && serve",
        "product-test": "yarn run build && yarn run serve"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 3 chrome version",
            "last 3 firefox version",
            "last 3 safari version"
        ]
    },
