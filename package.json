    "scripts": {
        "build": "gatsby build",
        "develop": "gatsby develop",
        "start": "yarn run develop",
        "gatsby-serve": "gatsby serve",
        "serve": "serve -s public",
        "clean": "gatsby clean",
        "type-check": "tsc --noEmit -p .",
        "format": "prettier --write .",
        "lint": "eslint src/** --max-warnings 10",
