        "gen-scss-type": "yarn tsm src/",
        "serve-public": "pushd .\\public && serve",
        "product-test": "yarn run build && yarn run serve-public"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
